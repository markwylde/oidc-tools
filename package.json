{
  "name": "oidc-tools",
  "version": "1.0.0",
  "description": "OpenID Connect tools for token decoding and verification",
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "example": "node -r dotenv/config examples/basic-usage.js",
    "example:auth": "node -r dotenv/config examples/auth-flow.js"
  },
  "keywords": [
    "oidc",
    "jwt",
    "token",
    "authentication"
  ],
  "author": {
    "name": "Mark Wylde",
    "email": "mark@wylde.net",
    "url": "https://github.com/markwylde"
  },
  "license": "MIT",
  "dependencies": {
    "jose": "^6.0.10"
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "dotenv": "^16.4.7",
    "typescript": "^5.8.2"
  }
}
